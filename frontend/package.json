    "@sveltejs/kit": "^1.20.4",
    "@types/cookie": "^0.5.1",
    "@types/jsonwebtoken": "^9.0.3",
    "postcss": "^8.4.29",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "svelte": "^4.0.5",
    "svelte-adapter-bun": "^0.5.0",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.3",
