{
  "name": "svelte-sol-bun",
  "description": "Solana sveltekit starter app utilizing Bun as package manager",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build --config vite.build.config.ts",
    "preview": "vite preview -c vite.build.config.ts",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
